{
  "name": "next-export-optimize-images",
  "version": "1.5.1",
  "description": "Optimize images at build time with Next.js.",
  "keywords": [
    "next.js",
    "static",
    "export",
    "image",
    "optimization"
  ],
  "homepage": "https://next-export-optimize-images.vercel.app",
  "bugs": {
    "url": "https://github.com/dc7290/next-export-optimize-images/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dc7290/next-export-optimize-images.git"
  },
  "license": "MIT",
  "author": "dc7290 <dhkh.cba0927@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "swc src -d dist -w",
    "prebuild": "rimraf dist",
    "build": "swc src -d dist && tsc --project ./tsconfig.build.json",
    "coverage": "vitest run --coverage",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "pretest": "zx __tests__/e2e/script.mjs",
    "test": "vitest",
    "test:cli": "vitest __tests__/cli",
    "test:components": "vitest __tests__/components",
    "pretest:e2e": "yarn pretest",
    "test:e2e": "vitest run __tests__/e2e",
    "semantic-release": "SKIP_BY_SEMANTIC_RELEASE=true semantic-release",
    "typecheck": "yarn tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cli-progress": "^3.11.1",
    "fs-extra": "^10.1.0",
    "got": "^11.8.5",
    "lodash.uniqwith": "^4.5.0",
    "sharp": "^0.30.6"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.203",
    "@testing-library/react": "13.3.0",
    "@tsconfig/strictest": "1.0.1",
    "@types/benchmark": "2.1.1",
    "@types/cli-progress": "3.11.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash.uniqwith": "4.5.7",
    "@types/node": "16.11.41",
    "@types/react": "18.0.14",
    "@types/rimraf": "^3.0.2",
    "@types/sharp": "0.30.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "benchmark": "2.1.4",
    "c8": "7.11.3",
    "chokidar": "3.5.3",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "git-cz": "4.9.0",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.2",
    "next": "12.1.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.3",
    "typescript": "4.7.3",
    "vitest": "0.15.1",
    "zx": "7.0.0"
  },
  "peerDependencies": {
    "next": ">=12.0.0",
    "react": "^17.0.2 || ^18.0.0-0",
    "react-dom": "^17.0.2 || ^18.0.0-0"
  },
  "engines": {
    "node": "^16.0.0 || ^18.0.0"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
